<application>
  <component name="CodeFoldingSettings">
    <option name="COLLAPSE_METHODS" value="true" />
  </component>
  <component name="CodeInsightSettings">
    <option name="REFORMAT_ON_PASTE" value="1" />
    <option name="INDENT_TO_CARET_ON_PASTE" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
  </component>
  <component name="JSCodeFoldingSettings">
    <option name="collapseObjectLiterals" value="true" />
    <option name="COLLAPSE_OBJECT_LITERALS" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern useCustomColors="true" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="e81600" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="e81600" />
    </pattern>
    <pattern useCustomColors="true" pattern="\bnote\b.*">
      <option name="FOREGROUND" value="34b658" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="34b658" />
    </pattern>
  </component>
</application>